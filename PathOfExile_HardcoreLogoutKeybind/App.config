<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <!-- use this reference: -->
    <!-- https://docs.microsoft.com/en-us/dotnet/api/system.windows.forms.keys?view=netframework-4.7.2 -->
    <add key="kill_key" value="192"/>     <!-- ~ -->
    <add key="hideout_key" value="115"/>  <!-- F5 -->
    <add key="hideout_key" value="114"/>  <!-- F4 -->
    <add key="hideout_key" value="113"/>  <!-- F3 -->
  </appSettings>
